    "dotenv": "^16.1.3",
    "expo": "~48.0.18",
    "expo-constants": "~14.2.1",
    "expo-status-bar": "~1.4.4",
    "firebase": "^9.22.1",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-gifted-chat": "^2.1.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "twrnc": "^3.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "concurrently": "^8.0.1",
